<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BattleGridView">
        <attr name="background_cell" format="reference" />
        <attr name="frame_cell" format="reference" />
        <attr name="selection_cell" format="reference" />
        <attr name="selection_cell_wrong" format="reference" />
        <attr name="hitted_cell" format="reference" />
        <attr name="missed_cell" format="reference" />
        <attr name="font" format="reference" />
        <attr name="text_color" format="color"/>

    </declare-styleable>

    <declare-styleable name="ShipView">
        <attr name="lenght" format="enum">
            <enum name="ONE" value="1"/>
            <enum name="TWO" value="2"/>
            <enum name="THREE" value="3"/>
            <enum name="FOUR" value="4"/>
        </attr>
    </declare-styleable>

</resources>